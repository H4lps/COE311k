    "import numpy as np\n",
    "def time_series(n : int) -> float:\n",
    "    res = 0\n",
    "    for i in range(n):\n",
    "        res += ((-1) ** i) / ((2 * i) + 1)\n",
    "    return res * 4"
   ]
